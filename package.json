{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "prepare": "husky install",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "build:remix": "cross-env NODE_ENV=production dotenv -e .env remix build --sourcemap",
    "build:server": "node ./other/build-server.js",
    "build": "npm run build:css:prod && npm run build:remix",
    "dev:pm2": "pm2-dev ./other/pm2.config.js",
    "dev": "concurrently \"npm:dev:tailwind\" \"npm:dev:web\"",
    "dev:tailwind": "tailwindcss --output ./app/styles/tailwind.css --config ./tailwind.config.js --watch",
    "dev:web": "remix dev",
    "postinstall": "remix setup node",
    "deploy": "fly deploy --remote-only",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.4.0",
    "@xstate/inspect": "^0.5.2",
    "@xstate/react": "^1.6.3",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "error-stack-parser": "^2.0.6",
    "esbuild": "^0.14.5",
    "express": "^4.17.2",
    "express-http-proxy": "^1.6.3",
    "framer-motion": "^6.2.1",
    "md5-hash": "^1.0.1",
    "morgan": "^1.10.0",
    "on-finished": "^2.3.0",
    "pm2": "^5.1.2",
    "postcss-import": "^14.0.2",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redis": "^3.1.2",
    "remix": "^1.1.3",
    "tailwindcss": "^3.0.15",
    "xstate": "^4.26.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/estree": "0.0.50",
    "@types/express-http-proxy": "^1.6.3",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/lru-cache": "^5.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/on-finished": "^2.3.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/redis": "^2.8.32",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.5.1",
    "cypress": "^9.1.1",
    "dotenv-cli": "^4.1.1",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.2.1",
    "eslint": "^7.32.0",
    "eslint-config-kentcdodds": "^19.2.0",
    "faker": "^5.5.3",
