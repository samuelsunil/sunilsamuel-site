    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-watch-typeahead": "^0.6.4",
    "msw": "^0.36.3",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*"
  ]
}
